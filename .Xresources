Xft*dpi: 96
Xft*antialias: true
Xft*hinting: true
Xft*hintstyle: hintfull
Xft*rgba: rgb
!! Interface 
URxvt*scrollBar: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypass: true
URxvt.secondaryScreen: 1
URxvt.secondaryWheel: 1
URxvt.secondaryScroll: 0
*.internalBorder: 10
!! Font

URxvt.font: xft:DejaVu Sans Mono:pixelsize=12
URxvt.letterSpace: -1

!! Disable printing
URxvt.print-pipe: "cat > /dev/null"

!! Perl extensions

URxvt.perl-ext-common: default,matcher,selection-to-clipboard,url-select,tabbed

!!
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
!!
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.underline: true
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.colorUL: #4682B4
!!

URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list

!!
URxvt.tabbed.autohide: true
URxvt.keysym.Control-t: perl:tabbed:new_tab
URxvt.keysym.Control-Tab: perl:tabbed:next_tab
URxvt.keysym.Control-Shift-Tab: perl:tabbed:prev_tab


